background yes
use_xft yes
xftfont monospace:size=9
xftalpha 1
update_interval 2.0
total_run_times 0
own_window no
own_window_colour 222222
own_window_transparent no
own_window_argb_visual true
own_window_argb_value 170
own_window_type desktop
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
double_buffer yes
minimum_size 250 400
maximum_width 1000
draw_shades yes
draw_outline no
draw_borders no
draw_graph_borders no
default_color d9d7d6
default_shade_color 000000
default_outline_color d9d7d6
alignment top_right
gap_x 0
gap_y 0 
no_buffers yes
uppercase no
cpu_avg_samples 2
override_utf8_locale no
##############################################
#  Output
##############################################
TEXT
${color green}Net:${color}${upspeed eth0}/${downspeed eth0} _ ${color green}CPU:${color}(${cpu cpu1}%)(${cpu cpu2}%) _ ${color green}RAM:${color}${mem}/${memmax} _ ${color green}Load:${color}${loadavg} 
